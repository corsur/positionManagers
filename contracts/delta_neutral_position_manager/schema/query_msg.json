{
  "$schema": "http://json-schema.org/draft-07/schema#",
  "title": "QueryMsg",
  "description": "Get basic information for a position.",
  "oneOf": [
    {
      "type": "object",
      "required": [
        "get_position_info"
      ],
      "properties": {
        "get_position_info": {
          "type": "object",
          "required": [
            "position_id"
          ],
          "properties": {
            "position_id": {
              "type": "integer",
              "format": "uint64",
              "minimum": 0.0
            }
          }
        }
      },
      "additionalProperties": false
    }
  ]
}
